{
  "expo": {
    "name": "flipo-app",
    "description": "This project is really great.",
    "slug": "flipo-app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Flippo requires the camera to take an image to be identified",
        "NSPhotoLibraryAddUsageDescription": "Flippo access to your photo libraries"
      }
    }
  }
}
